record(calc,"$(P)VAC:D:Cols") {
    field(DESC, "D hutch upstream support (collimator slits) vac (torr)")
    field(SCAN,"1 second")
    field(CALC,"(A-FLOOR(A))*10**(FLOOR(A)-6)")
    field(INPA,"$(P)IP330_05.VAL")
    field(PREC,8)
}

